.content ul li:before {
    content: '';
    width: 6px;
    height: 6px;
    border-radius: 6px;
    background: #aa6929;
    position: absolute;
    left: 0;
    top: 6px;
}
.content blockquote {
    width: 100%;
    background: #82cdd1; /* Old browsers */
    background: -moz-linear-gradient(top,  #82cdd1 0%, #65c1c6 35%, #54babf 64%, #50b9be 74%, #50b9be 100%); /* FF3.6-15 */
    background: -webkit-linear-gradient(top,  #82cdd1 0%,#65c1c6 35%,#54babf 64%,#50b9be 74%,#50b9be 100%); /* Chrome10-25,Safari5.1-6 */
    background: linear-gradient(to bottom,  #82cdd1 0%,#65c1c6 35%,#54babf 64%,#50b9be 74%,#50b9be 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#82cdd1', endColorstr='#50b9be',GradientType=0 ); /* IE6-9 */
