    font: normal 14px/1.4 'Arial', sans-serif;
    padding-left: 15px;
}
.content ul ol li:before { font-size: 1em; line-height: normal; }

.content ol, .content ul ol { counter-reset: list; }
.content ol ol { counter-reset: sublist; }
.content ol ol ol { counter-reset: thirdlist; }

.content ol li:before, .content ul ol li:before { font-weight: 400; counter-increment: list; content: counter(list) ".";  color: #aa6929;position: absolute; left: 0px; }
.content ol li li:before{ counter-increment: sublist; content: counter(list) "." counter(sublist) "."; }
.content ol li li li:before { counter-increment: thirdlist; content: counter(list) "." counter(sublist) "." counter(thirdlist) "."; }

.content ol li li { padding-left: 32px; }
.content ol li li li { padding-left: 44px; }

section h1, .content section h1 {
    font: bold 3.2em/1.2 'Akrobat', arial, sans-serif;
