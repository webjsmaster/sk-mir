.staticPageMenu {
    display: block;
    text-align: right;
    list-style: none;
    padding: 0 0 10px 0;
}
.staticPageMenu li {
    display: inline-block;
    vertical-align: middle;
}
.staticPageMenu li a {
    color: #475b5f;
    font: bold 1.6em/1.4 'Akrobat', sans-serif;
    text-decoration: none;
    text-transform: uppercase;
    letter-spacing: 0.5px;
    padding: 8px 20px;
    position: relative;
}
.staticPageMenu li a:after {
    content: '';
    width: 1px;
    height: 18px;
    position: absolute;
    left: 0;
    top: 50%;
    margin-top: -9px;
    background: url(../../img/sprite.png) bottom right no-repeat;
}
.staticPageMenu li a:hover, .staticPageMenu li.current a {
    background: #e4a364;
    color: #fff;
}
.staticPageMenu li a:hover:after, .staticPageMenu li.current a:after {
    content: none;
}
.staticPageMenu li:hover + li a:after, .staticPageMenu li.current + li a:after {
    content: none;
}
.staticPageMenu li:first-child a:after {
    content: none;
}

.frameLink {
    width: 100%;
    height: 100%;
    display: block;
    position: absolute;
    left: 0;
    top: 0;
}

table.tableInfo {
