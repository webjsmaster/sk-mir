}
table.tableInfo .section {
    font: bold 1.4em/1.4 'Akrobat', sans-serif;
    text-transform: uppercase;
    letter-spacing: 0.5px;
    display: block;
}
table.tableInfo tr th {
    font: bold 1.4em/1.4 'Akrobat', sans-serif;
    text-transform: uppercase;
    letter-spacing: 0.5px;
    padding: 11px 10px;
    background: #fdf8f3;
    border: 1px solid #e2d5c8;
}
table.tableInfo tr td .big {
    display: inline-block;
    vertical-align: middle;
    font: bold 2.8em/1.4 'Akrobat', sans-serif;
    margin: 0 5px;
}
table.tableInfo tr td {
    padding: 7px 5px;
