/*-------- Header ---------*/
header .topBlock {
    background: #007b81;
    height: 50px;
    box-shadow: 0 1px 6px rgba(0,0,0,0.4);
    position: relative;
    z-index: 2;
}
header .topBlock .col-md-3:nth-child(2) .phone {
    margin-left: 13px;
}
header .topBlock .phone {
    font: normal 1.8em/1.4 'Akrobat', sans-serif;
    color: #fff;
    position: relative;
    padding-left: 22px;
    letter-spacing: 0.5px;
    display: block;
    margin-top: 11px;
}
header .topBlock .mail {
    font: normal 1.8em/1.4 'Akrobat', sans-serif;
    color: #fff;
    position: relative;
    padding-left: 32px;
    letter-spacing: 0.5px;
    display: block;
    margin-top: 11px;
    text-decoration: none;
    margin-left: 30px;
}
header .topBlock .phone:before {
    content: '';
    position: absolute;
    left: 0;
    top: 3px;
    width: 7px;
    height: 18px;
    background: url(../../img/sprite.png) -20px 0 no-repeat;
}
header .topBlock .mail:before {
    content: '';
    position: absolute;
    left: 0;
    top: 7px;
    width: 18px;
    height: 13px;
    background: url(../../img/sprite.png) 0 0 no-repeat;
}
header .logo {
    display: table;
    width: 180px;
    height: 170px;
    position: absolute;
    z-index: 10;
    background: #0e9298;
}
header .logo a {
    display: table-cell;
    vertical-align: middle;
    text-align: center;
    width: 100%;
}

header .menuContainer {
    position: relative;
    z-index: 1;
    background: #475b5f;
}
.mainMenu {
    padding-left: 25px;
    width: 100%;
    display: table;
    margin: 0;
}
.mainMenu > li {
    display: table-cell;
    vertical-align: middle;
    font: bold 1.9em/1.4 'Akrobat', sans-serif;
    position: relative;
}
.mainMenu > li:after {
    content: '';
    width: 1px;
    height: 30px;
    position: absolute;
    right: 0;
    top: 50%;
    margin-top: -15px;
    background: url(../../img/sprite.png) -99px 0 no-repeat;
}
.mainMenu > li:hover:after, .mainMenu > li.active:after, .mainMenu > li.current:after {
    content: none;
}
.mainMenu > li:last-child:after {
    content: none;
}
.mainMenu > li > a {
    width: 100%;
    text-decoration: none;
    color: #fff;
    text-align: center;
    display: block;
    text-transform: uppercase;
    letter-spacing: 0.5px;
    padding: 29px 14px;
    position: relative;
}
.mainMenu > li.active > a:after {
    content: '';
    position: absolute;
    bottom: 0;
    left: 30%;
    width: 0;
    height: 0;
    border-style: solid;
    border-width: 0 8.5px 9px 8.5px;
    border-color: transparent transparent #ffeedd transparent;
}
.mainMenu > li > a:hover, .mainMenu > li.active > a, .mainMenu > li.current > a {
    background: #e4a364;
}
.mainMenu > li.active > .subMenu {
    display: table;
}
.mainMenu > li > .subMenu {
    position: absolute;
    left: 0;
    top: 100%;
    background: #ffeedd;
    padding: 25px;
    display: none;
}
.mainMenu > li > .subMenu > ul {
    font-size: 10px;
    list-style: none;
    display: table-cell;
    vertical-align: top;
    width: 250px;
    padding-right: 20px;
}
.mainMenu > li > .subMenu > ul > li {
    width: 250px;
    margin-bottom: 8px;
}
.mainMenu > li > .subMenu > ul > li > a {
    display: block;
    color: #152022;
    font: normal 1.4em/1.4 'Akrobat', sans-serif;
    text-decoration: none;
}
.mainMenu > li > .subMenu > ul > li > a:hover {
    color: #007b81;
}
.mainMenu > li > .subMenu > .plate {
    display: table-cell;
    vertical-align: middle;
    width: 160px;
    padding: 0 30px;
    border-left: 1px solid #decbb9;
}
.mainMenu > li > .subMenu > .plate a {
    text-decoration: none;
}
.mainMenu > li > .subMenu > .plate a span {
    display: block;
    color: #bc7735;
    font-size: 18px;
    font-weight: bold;
    width: 140px;
    margin: 0 auto;
    text-align: center;
    line-height: 1.1;
}
.mainMenu > li > .subMenu > .plate a:hover span {
    color: #9f662d;
}
.mainMenu > li > .subMenu > .plate img {
    margin-bottom: 5px;
    max-width: 120px
}

