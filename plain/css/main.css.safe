/*------------ Reset --------------*/
* { margin: 0; outline: none; }
body, p, ul, li, ol, pre, caption, h1, h2, h3, h4, h5, h6, form, div, fieldset, legend { padding: 0; }
h1, h2, h3, h4, h5, h6 {  }
[data-cbox] { outline: none; }
body { display: block; position: relative; min-height: 100%; width: 100%; min-width: 960px; font: normal 1em/1 'Arial', sans-serif; background: #fff; background-size: cover; z-index: -10000; }
img, fieldset { border: 0; }
input::-moz-focus-inner, button::-moz-focus-inner { padding: 0; border: 0; }
a { color: #007b81; text-decoration: none; }
a:hover { color: #000; text-decoration: none; }


/*--------------- Slick slider --------------*/
.slick-arrow {
    position: absolute;
    width: 65px;
    height: 65px;
    left: 40px;
    top: 50%;
    margin-top: -32px;
    border: none;
    text-indent: -9999px;
    overflow: hidden;
    z-index: 10;
    background: rgba(0,0,0,0.3);
    border-radius: 50%;
}
.slick-arrow:after {
    content: '';
    width: 19px;
    height: 34px;
    position: absolute;
    left: 50%;
    top: 50%;
    margin-top: -17px;
    margin-left: -13px;
    background: url(../../img/sprite.png) 0 -66px no-repeat;
}
.slick-arrow.slick-next {
    left: auto;
    right: 40px;
    transform: rotate(180deg);
}
.slick-arrow:hover {
    background: rgba(0,0,0,0.8);
}
.slick-disabled {
    display: none !important;
}


